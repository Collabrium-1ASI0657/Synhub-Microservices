# =============================================
# DATABASE CONFIGURATION
# =============================================

# PostgreSQL connection URL
# Format: jdbc:postgresql://host:port/database_name
POSTGRES_URL=jdbc:postgresql://localhost:5432/synhub-iam

# PostgreSQL username
POSTGRES_USER=your_database_username_here

# PostgreSQL password
POSTGRES_PASSWORD=your_database_password_here


# =============================================
# JWT CONFIGURATION
# =============================================

# Secret key for JWT token signing and validation
# Must be at least 32 characters long for security
# Must be the same as the one in API-GATEWAY
JWT_SECRET=your_super_secure_jwt_secret_key_min_32_chars